{
  "name": "juttle-sqlite-adapter",
  "version": "0.3.1",
  "description": "Juttle adapter for SQLite",
  "keywords": [
    "juttle",
    "adapter",
    "sqlite"
  ],
  "homepage": "https://github.com/juttle/juttle-sqlite-adapter",
  "bugs": "https://github.com/juttle/juttle-sqlite-adapter/issues",
  "license": "Apache-2.0",
  "repository": "juttle/juttle-sqlite-adapter",
  "scripts": {
