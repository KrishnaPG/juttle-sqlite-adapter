  },
  "devDependencies": {
    "chai": "^1.10.0",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  },
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=2.14.7"
  }
}
